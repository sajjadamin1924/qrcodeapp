{
  "expo": {
    "name": "QR CODE",
    "slug": "qrcodeapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splashicondark.png",
    "scheme": "qrcodeapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "image": "./assets/images/splashiconlight.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFF00",
      "dark": {
        "image": "./assets/images/splashicondark.png",
        "backgroundColor": "#000000"
      }
    },
    "plugins": [
      "expo-router",
      "expo-barcode-scanner"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
